{
  "name": "@jaypie/core",
  "version": "0.4.3",
  "author": "Finlayson Studio",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "init:deploy": "hygen jaypie workflow-npm",
    "lint": "eslint .",
    "new": "hygen jaypie vite",
    "test": "vitest",
    "test:spec:Client": "vitest run ./src/__tests__/Client.spec.js",
    "test:spec:constants": "vitest run ./src/core/__tests__/constants.spec.js",
    "test:spec:core": "vitest run ./src/__tests__/core.spec.js",
    "test:spec:express:decorateResponse.function": "vitest run ./src/express/__tests__/decorateResponse.function.spec.js",
    "test:spec:express:getCurrentInvokeUuid.adapter": "vitest run ./src/express/__tests__/getCurrentInvokeUuid.adapter.spec.js",
    "test:spec:express:summarizeRequest.function": "vitest run ./src/express/__tests__/summarizeRequest.function.spec.js",
    "test:spec:express:summarizeResponse.function": "vitest run ./src/express/__tests__/summarizeResponse.function.spec.js",
    "test:spec:expressHandler.module": "vitest run ./src/__tests__/expressHandler.module.spec.js",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.js",
    "test:spec:jaypieHandler.module": "vitest run ./src//__tests__/jaypieHandler.module.spec.js",
    "test:spec:knowdev.lib": "vitest run ./src/core/__tests__/knowdev.lib.spec.js",
    "test:spec:lambdaHandler.module": "vitest run ./src/__tests__/lambdaHandler.module.spec.js",
    "test:spec:lib:client:index": "vitest run ./test/client/__tests__/index.spec.js",
    "test:spec:logTags.function": "vitest run ./src/core/__tests__/logTags.function.spec.js",
    "test:spec:logger.module": "vitest run ./src/core/__tests__/logger.module.spec.js"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.13.0",
    "@knowdev/arguments": "^1.3.0",
    "@knowdev/errors": "^2.0.4",
    "@knowdev/functions": "^1.0.0",
    "@knowdev/http": "^1.4.0",
    "@knowdev/log": "^1.0.2",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.3.22",
    "hygen": "^6.2.11",
    "jest-extended": "^4.0.2",
    "jest-json-schema": "^6.1.0",
    "mock-express-response": "^0.3.0",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.8.0",
    "vitest": "^1.3.1"
  }
}
